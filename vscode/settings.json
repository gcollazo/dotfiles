{
  "editor.codeActionsOnSave": {
    "source.organizeImports": true
  },
  "editor.fontFamily": "Cascadia Code",
  "editor.fontSize": 19,
  "editor.formatOnSave": true,
  "editor.lineNumbers": "off",
  "editor.minimap.enabled": false,
  "editor.renderIndentGuides": true,
  "editor.rulers": [80, 100],
  "editor.tabCompletion": "on",
  "editor.tabSize": 2,
  "workbench.colorTheme": "Night Owl Light (No Italics)",
  "workbench.iconTheme": "material-icon-theme",
  "workbench.startupEditor": "newUntitledFile",
  "terminal.integrated.fontSize": 17,
  "terminal.integrated.shell.osx": "/bin/zsh",
  "window.zoomLevel": 0,
  "git.autofetch": true,
  "git.confirmSync": false,
  "git.enableSmartCommit": true,
  "gitlens.codeLens.authors.enabled": false,
  "gitlens.codeLens.enabled": false,
  "gitlens.advanced.telemetry.enabled": false,
  "githubPullRequests.telemetry.enabled": false,
  "telemetry.enableTelemetry": false,
  "telemetry.enableCrashReporter": false,
  "eslint.packageManager": "yarn",
  "prettier.arrowParens": "always",
  "python.formatting.blackArgs": ["--line-length", "88"],
  "python.formatting.blackPath": "/Users/gcollazo/.local/bin/black",
  "python.formatting.provider": "black",
  "python.linting.flake8Enabled": true,
  "python.linting.flake8Path": "/Users/gcollazo/.local/bin/flake8",
  "python.linting.pylintEnabled": false,
  "python.sortImports.path": "/Users/gcollazo/.local/bin/isort",
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": true,
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/*.pyc": true
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[handlebars]": {
    "editor.formatOnSave": false
  },
  "[python]": {
    "editor.tabSize": 4
  },
  "cSpell.language": "en,es",
  "cSpell.enabledLanguageIds": [
    "asciidoc",
    "c",
    "cpp",
    "csharp",
    "css",
    "git-commit",
    "go",
    "handlebars",
    "html",
    "jade",
    "java",
    "javascript",
    "javascriptreact",
    "json",
    "jsonc",
    "latex",
    "less",
    "markdown",
    "php",
    "plaintext",
    "pug",
    "restructuredtext",
    "rust",
    "scala",
    "scss",
    "text",
    "typescript",
    "typescriptreact",
    "yaml",
    "yml"
  ]
}
