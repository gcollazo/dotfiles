{
  "editor.fontFamily": "Inconsolata",
  "editor.fontSize": 21,
  "editor.lineHeight": 25,
  "editor.tabCompletion": true,
  "editor.tabSize": 2,
  "editor.minimap.enabled": false,
  "editor.renderIndentGuides": true,
  "editor.formatOnSave": true,
  "editor.rulers": [80, 100],
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "extensions.ignoreRecommendations": false,
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": true,
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/*.pyc": true
  },
  "window.zoomLevel": 0,
  "terminal.integrated.fontSize": 16,
  "terminal.integrated.shellArgs.osx": ["-l"],
  "git.enableSmartCommit": true,
  "git.confirmSync": false,
  "git.autofetch": true,
  "prettier.singleQuote": true,
  "prettier.arrowParens": "always",
  "python.formatting.provider": "black",
  "python.formatting.blackPath": "/Users/gcollazo/.local/bin/black",
  "python.formatting.blackArgs": ["--line-length", "88"],
  "python.linting.flake8Enabled": true,
  "python.linting.flake8Path": "/Users/gcollazo/.local/bin/flake8",
  "python.linting.flake8Args": ["--max-line-length=88"],
  "python.linting.pylintEnabled": false,
  "python.sortImports.path": "/Users/gcollazo/.local/bin/isort",
  "[python]": {
    "editor.tabSize": 4
  },
  "[handlebars]": {
    "editor.formatOnSave": false
  },
  "[html]": {
    "editor.formatOnSave": false,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "workbench.colorTheme": "Material Theme Palenight High Contrast",
  "workbench.startupEditor": "newUntitledFile",
  "workbench.colorCustomizations": {},
  "material-icon-theme.hidesExplorerArrows": true,
  "material-icon-theme.activeIconPack": "",
  "material-icon-theme.folders.theme": "classic",
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "material-icon-theme.saturation": 1,
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "eslint.packageManager": "yarn",
  "telemetry.enableTelemetry": false,
  "telemetry.enableCrashReporter": false,
  "cSpell.enabledLanguageIds": [
    "asciidoc",
    "c",
    "cpp",
    "csharp",
    "css",
    "git-commit",
    "go",
    "handlebars",
    "html",
    "jade",
    "java",
    "javascript",
    "javascriptreact",
    "json",
    "jsonc",
    "latex",
    "less",
    "markdown",
    "php",
    "plaintext",
    "pug",
    "restructuredtext",
    "rust",
    "scala",
    "scss",
    "text",
    "typescript",
    "typescriptreact",
    "yaml",
    "yml"
  ],
  "gitlens.advanced.telemetry.enabled": false,
  "githubPullRequests.telemetry.enabled": false,
  "workbench.iconTheme": "material-icon-theme"
}
