{
  "extends": "ember",
  "env": {
    "node": true,
    "browser": true,
    "es6": true
  },
  "rules": {
    "arrow-spacing": [2, { "before": true, "after": true }],
    "block-scoped-var": 0,
    "brace-style": 0,
    "consistent-return": 0,
    "consistent-this": 0,
    "guard-for-in": 0,
    "max-len": [2, 250],
    "max-nested-callbacks": [2, 5],
    "newline-after-var": 0,
    "no-console": 0,
    "no-else-return": 0,
    "no-extend-native": 0,
    "no-invalid-this": 0,
    "no-multiple-empty-lines": 0,
    "no-param-reassign": 0,
    "no-process-env": 0,
    "no-restricted-syntax": [0, "FunctionExpression"],
    "no-shadow": 0,
    "no-ternary": 0,
    "no-undefined": 0,
    "no-underscore-dangle": 0,
    "no-use-before-define": 0,
    "no-warning-comments": 0,
    "object-curly-spacing": [2, "always"],
    "one-var": 0,
    "quotes": 0,
    "quotes": [2, "single", "avoid-escape"],
    "space-before-function-paren": 0,
    "space-in-parens": 0,
    "spaced-line-comment": 0,
    "valid-jsdoc": 0
  }
}
